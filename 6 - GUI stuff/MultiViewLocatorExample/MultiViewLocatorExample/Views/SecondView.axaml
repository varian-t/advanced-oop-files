<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:MultiViewExample.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="vm:SecondViewModel"
             x:Class="MultiViewExample.Views.SecondView">
            <StackPanel>
                <TextBlock Text="{Binding SecondViewText}"/>
                <Image Width="1200" Source="/Assets/cat_with_glasses1.jpg"/>
            </StackPanel>
</UserControl>
