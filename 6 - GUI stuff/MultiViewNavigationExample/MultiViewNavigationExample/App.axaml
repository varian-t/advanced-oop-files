<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="MultiViewExample.App"
             xmlns:local="using:MultiViewExample"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
  
    <Application.Styles>
        <SimpleTheme/>
    </Application.Styles>

    <Application.Resources>
        <FontFamily x:Key="FontAwesomeRegular">avares://MultiViewExample/Assets/Fonts/#Font Awesome 6 Free Regular</FontFamily>
        <FontFamily x:Key="FontAwesomeSolid">avares://MultiViewExample/Assets/Fonts/#Font Awesome 6 Free Solid</FontFamily>
    </Application.Resources>
</Application>